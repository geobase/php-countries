{
  "name": "geobase/countries",
  "type": "library",
  "description": "Data from Open Data providers compiled into easy to use PHP objects",
  "keywords": ["Country", "Region", "Data", "Database", "GeoIp", "Geolocation", "Province", "State"],
  "homepage": "https://github.com/geobase/php-countries",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Bull",
      "email": "me@gabrielbull.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "league/geotools": ">=0.6",
    "geobase/countries-data": "^1.0",
    "phpunit/phpunit": "^6.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "GeoBase\\Countries\\": "src",
      "GeoBase\\Regions\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GeoBase\\Countries\\Tests\\": [
        "tests/Countries/Tests",
        "tests/Countries/Tests/_includes"
      ]
    }
  }
}
